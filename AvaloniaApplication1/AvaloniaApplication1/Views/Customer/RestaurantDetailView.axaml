<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:AvaloniaApplication1.ViewModels.Customer"
             mc:Ignorable="d" d:DesignWidth="384" d:DesignHeight="832"
             x:Class="AvaloniaApplication1.Views.Customer.RestaurantDetailView"
             x:DataType="vm:RestaurantDetailViewModel">
    
    <StackPanel Initialized="Panel_OnInitialized">
        <ComboBox Name="RestaurantNamesComboBox"></ComboBox>
        <ComboBox Name="FoodNamesComboBox"></ComboBox>
        <ComboBox Name="DrinkNamesComboBox"></ComboBox>
        <Button Click="SavePreference_OnClick">Ulozit preferenci</Button>
    </StackPanel>
</UserControl>
